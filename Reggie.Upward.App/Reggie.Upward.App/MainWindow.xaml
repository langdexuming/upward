<Window x:Class="Reggie.Upward.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Reggie.Upward.App"
        xmlns:controls="clr-namespace:Reggie.Upward.App.Controls"
        mc:Ignorable="d"
        Title="Upward" SizeToContent="Height" Width="525"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
    <Viewbox Stretch="Uniform">
        <Grid Width="525" x:Name="gdRoot">
            <WebBrowser x:Name="webBrowser"></WebBrowser>

            <WindowsFormsHost x:Name="windowsFormsHost">
                
            </WindowsFormsHost>


        </Grid>
    </Viewbox>
</Window>
